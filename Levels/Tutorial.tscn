[sub_resource type="GDScript" id=1]
script/source = "extends Node

const DECIDE = preload(\"res://Level_Decide.tscn\")

onready var root = get_parent()
onready var player = root.get_node(\"Player\")
onready var camera = player.get_node(\"Camera2D\")

func leave():
	player.leaving = true
	
	root.temple = false
	root.level = 1
	root.save_data()
	
	var decide = DECIDE.instance()
	add_child(decide)
	decide.decide_location()

func _ready():
	var CAMERA_X = 5
	var CAMERA_Y = 1
	
	camera.limit_bottom = 720 * CAMERA_Y
	camera.limit_right = 1280 * CAMERA_X
	player.no_death = true
"

